# Preview Environment Configuration
# This file is used for Render preview deployments
# Copy this to .env.preview and fill in the values for local preview testing

# Environment identifier
ENVIRONMENT=preview

# Port (Render sets this automatically)
PORT=10000

# Database (auto-configured by Render from autonomos-db-preview)
DATABASE_URL=postgresql://user:pass@host:5432/autonomos_preview

# Redis (auto-configured by Render from autonomos-redis-preview)
REDIS_URL=redis://host:6379

# API Keys (set these in Render dashboard for preview environment)
GEMINI_API_KEY=your_gemini_key_here
PINECONE_API_KEY=your_pinecone_key_here
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX=schema-mappings-e5

# Auto-generated by Render for preview
SECRET_KEY=auto_generated
API_KEY=auto_generated

# Auto-configured by Render
ALLOWED_WEB_ORIGIN=https://autonomos-platform-preview-xxx.onrender.com

# Optional
SLACK_WEBHOOK_URL=your_slack_webhook_url

# Migration settings
DISABLE_AUTO_MIGRATIONS=false

# Feature flags for preview
DEV_DEBUG=true
FEATURE_USE_FILESOURCE=true
REQUIRED_SOURCES=salesforce,supabase,mongodb,filesource

# JWT settings
JWT_EXPIRE_MINUTES=30
